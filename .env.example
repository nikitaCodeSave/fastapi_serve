# Конфигурация vLLM API
# Скопируйте этот файл как .env и настройте значения под ваши нужды

# Модель vLLM
VLLM_MODEL=/home/nikita/PROJECTS/vllm5090/project/vikhr/models/QVikhr-3-1.7B

# Сетевые настройки
HOST=0.0.0.0
PORT=8000

# Отладка
DEBUG=false
LOG_LEVEL=info

# API
API_PREFIX=v1

# Дефолтные параметры генерации (используются если не указано в запросе)
MAX_TOKENS=8192
TEMPERATURE=0.2
TOP_P=0.95

# Настройки vLLM
GPU_MEMORY_UTILIZATION=0.5
TENSOR_PARALLEL_SIZE=1